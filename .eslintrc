{
	"extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "classes": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
	"rules": {
		"react/jsx-filename-extension": 0,
		"no-underscore-dangle": 0,
		"consistent-return": 0
	},
	"globals": {
		"document": true,
		"window": true,
		"describe": true,
		"beforeEach": true,
		"test": true,
		"expect": true,
		"module": true,
		"fetch": true,
		"__CLIENT__": true,
		"__SERVER__": true,
		"__DEV__": true
	},
	"plugins": [
		"react",
		"import"
	]
}
