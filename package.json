{
  "name": "react-universal-boilerplate",
  "version": "1.0.0",
  "description": "A starter boilerplate for an universal web application with the best development experience and best practices.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdenisov/react-universal-boilerplate"
  },
  "keywords": [
    "starter",
    "boilerpate",
    "universal",
    "react",
    "redux",
    "react router v4",
    "koa",
    "webpack v3",
    "es6",
    "code splitting",
    "hot reloading",
    "babel",
    "postcss",
    "jest",
    "unit testing",
    "performance optimizing"
  ],
  "main": "server/index.js",
  "engines": {
    "node": ">=7.0",
    "npm": ">=3.0"
  },
  "author": "Maxim Denisov <maxim@denisov.in> (https://github.com/mdenisov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdenisov/react-universal-boilerplate/issues"
  },
  "homepage": "https://github.com/mdenisov/react-universal-boilerplate",
  "scripts": {
    "test": "cross-env NODE_ENV=test echo 'Running tests' && jest --forceExit",
    "lint:js": "echo 'Running linter' && eslint \"server/**/*.js\" \"shared/**/*.js\" \"client/**/*.js\" --fix",
    "lint:css": "echo 'Running linter' && stylelint \"shared/**/*.css\" --fix",
    "clean": "rimraf ./public/dist",
    "dev": "npm run build:dev && npm run start:dev",
    "build:dev": "cross-env NODE_ENV=development npm run clean && webpack --config ./tools/webpack/config.dev.js --progress --colors",
    "start:dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "prod": "npm run build:prod && npm run start:prod",
    "build:prod": "cross-env NODE_ENV=production npm run clean && webpack --config ./tools/webpack/config.prod.js --progress --colors",
    "start:prod": "cross-env NODE_ENV=production node tools/server/cluster.js"
  },
  "dependencies": {
    "boom": "^7.2.0",
    "boxen": "^1.3.0",
    "chalk": "^2.3.1",
    "compressible": "^2.0.13",
    "cross-env": "^5.1.3",
    "isomorphic-fetch": "^2.2.1",
    "kcors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-better-error-handler": "^1.3.5",
    "koa-better-timeout": "0.0.2",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-favicon": "^2.0.0",
    "koa-helmet": "^3.3.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.5",
    "os": "^0.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.4.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-css-modules-transform": "^1.4.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.10",
    "css-loader": "^0.28.10",
    "cssnano": "^3.10.0",
    "debug": "^3.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "image-webpack-loader": "^4.1.0",
    "jest": "^22.4.1",
    "jest-css-modules": "^1.1.0",
    "koa-logger": "^3.2.0",
    "koa-response-time": "^2.0.0",
    "koa-webpack-middleware": "^1.0.7",
    "nodemon": "^1.15.1",
    "postcss": "^6.0.19",
    "postcss-clean": "^1.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-nesting": "^4.2.1",
    "prop-types": "^15.6.0",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.6.2",
    "style-loader": "0.19.1",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "supertest": "^3.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
